[
  {
    type: "button"
    icon: "document"
    callback: "application:new-file"
    tooltip: "New File"
    iconset: "ion"
    style:
      color: "tan"
  }
  {
    type: "button"
    icon: "folder"
    iconset: "ion"
    tooltip: "Open..."
    callback: "application:open-file"
    style:
      color: "tan"
  }
  {
    type: "spacer"
  }
  {
    type: "button"
    icon: "navicon-round"
    iconset: "ion"
    tooltip: "Command Palette"
    callback: "command-palette:toggle"
  }
  {
    type: "button"
    icon: "circuit-board"
    callback: "git-diff:toggle-diff-list"
    tooltip: "Toggle Diff List"
    style:
      color: "#FA4F28"
  }
  {
    type: "button"
    icon: "tools"
    tooltip: "Macros"
    callback: "macros:edit-macros"
  }
  {
    type: "spacer"
  }
  {
    type: "button"
    icon: "markdown"
    tooltip: "Toggle markdown preview"
    callback: "markdown-preview:toggle"
  }
  # spacer to separate 'updated toolbar' button at end
  {
    type: "spacer"
  }
  {
    type: "button"
    icon: "book"
    iconset: "icomoon"
    tooltip: "Save As Project"
    callback: "project-manager:save-project"
    style:
      color: "slateblue"
  }
  {
    type: "button"
    icon: "books"
    iconset: "icomoon"
    tooltip: "Open Project"
    callback: "project-manager:list-projects"
    style:
      color: "slateblue"
  }
  {
    type: "spacer"
  }
]
